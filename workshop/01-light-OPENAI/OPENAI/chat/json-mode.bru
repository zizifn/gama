meta {
  name: json-mode
  type: http
  seq: 4
}

post {
  url: https://aigptx.top/v1/chat/completions
  body: json
  auth: bearer
}

headers {
  Content-Type: application/json
}

auth:bearer {
  token: {{KEY}}
}

body:json {
  {
    "model": "gpt-3.5-turbo",
    "response_format": { "type": "json_object" },
    "messages": [
      {
        "role": "system",
        "content": "You will be provided with text, and your task is to translate it into emojis. Do not use any regular text. Do your best with emojis only. Do output JSON."
      },
      {
        "role": "user",
        "content": "Artificial intelligence is a technology with great promise."
      }
    ],
    "temperature": 0.8,
    "max_tokens": 64,
    "top_p": 1
  }
}
